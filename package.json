{
  "name": "@epsor/apollo-client-wrapper",
  "version": "2.1.8",
  "author": "Epsor",
  "description": "GraphQL API Client for Epsor",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "test": "echo no tests",
    "start": "babel src -d dist --watch --copy-files --verbose",
    "clean": "rm -rf dist",
    "build": "babel src -d dist --copy-files --ignore 'src/**/*.test.js'",
    "commit": "git-cz",
    "lint": "eslint src --cache --max-warnings=0 --cache-location .eslintcache"
  },
  "types": "index.d.ts",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-ws": "^1.0.18",
    "apollo-upload-client": "^11.0.0",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.3.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "subscriptions-transport-ws": "^0.9.16",
    "unfetch": "^4.1.0",
    "uuid": "^3.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-eslint": "^10.0.2",
    "commitizen": "^4.0.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "git-cz": "^3.2.1",
    "husky": "^3.0.2",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  }
}
